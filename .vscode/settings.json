{
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk (quick preview)",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-cd",
        "-interaction=nonstopmode",
        "-synctex=1",
        "-file-line-error",
        "-auxdir=%WORKSPACE_FOLDER%/.showyourwork/compile/quick",
        "-outdir=%WORKSPACE_FOLDER%",
        "%DOC%"
      ]
    },
    {
      "name": "showyourwork (mamba)",
      "command": "mamba",
      "args": [
        "run",
        "-n",
        "syw-latest",
        "showyourwork",
        "build",
        "--project",
        "%WORKSPACE_FOLDER%",
        "--conda-frontend",
        "mamba"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "quick preview",
      "tools": ["latexmk (quick preview)"]
    },
    {
      "name": "showyourwork (mamba)",
      "tools": ["showyourwork (mamba)"]
    }
  ],
  "latex-workshop.latex.recipe.default": "quick preview",
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.latex.autoBuild.interval": 3000,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "%WORKSPACE_FOLDER%"
}